{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PowerBI": {
    "ApiUrl": "https://api.powerbi.com",
    "AuthorityBaseUrl": "https://login.microsoftonline.com",
    "ClientId": "<Client ID>", // Found in Azure Portal > App Registrations > Application ID
    "ClientSecret": "<Client Secret>", // Found in Azure Portal > App Registrations > Certificates & secrets
    "ResourceUrl": "https://analysis.windows.net/powerbi/api",
    "TenantId": "<Tenant ID>" // Your Azure Tenant ID
  }
}
